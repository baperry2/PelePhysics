ncells = 1 1 1
max_grid_size  = 1
# fuel_name = NC12H26
fuel_name = CH4
reactFormat = Array4
#reactFormat = 1dArray
#chem_integrator = "ReactorCvode"
chem_integrator = "ReactorRK64"

ode.dt  = 0.02
ode.ndt = 2000
ode.ode_ncells = 1
ode.reactor_type  = 2 # 1=full e, 2=full h
ode.verbose = 0
ode.use_typ_vals = 0
ode.rtol = 1e-10
ode.atol = 1e-10

# Select Linear solver for CVODE Newton direction
# Options are [CPU] dense_direct / denseAJ_direct / sparse_direct / custom_direct / GMRES / precGMRES
#             [GPU] sparse_direct / magma_direct / GMRES / precGMRES
cvode.solve_type = dense_direct

# If solve_type = precGMRES -> select preconditioner
# Options are [CPU] dense_simplified_AJacobian / sparse_simplified_AJacobian / custom_simplified_AJacobian
              [GPU] cuSparse_simplified_AJacobian
cvode.precond_type = dense_simplified_AJacobian
cvode.max_order = 4

ode.implicit_solve = 1
ode.use_erkstep    = 0

# amrex.fpe_trap_invalid = 1
# amrex.fpe_trap_zero = 1
# amrex.fpe_trap_overflow = 1


# 1013250.0 * patm
#hr.press = 5066250.0
hr.press = 1013250.0
#hr.t0 = 600
hr.t0 = 1400
#hr.equiv_ratio = 7.928571428571429
hr.equiv_ratio = 1.0
hr.outputFolderHR = ./ 

# Manifold settings
# manifold.model = NeuralNet
manifold.model = Table
# manifold.filename = cmlm_net.pnn
# manifold.filename = cmlm_net.pt
manifold.filename = autoignition.ctb
# manifold.info_filename = cmlm_net_info.txt
# manifold.cmlm_net = 1
manifold.v = 1

eos.nominal_pressure_cgs = 1013250.0
